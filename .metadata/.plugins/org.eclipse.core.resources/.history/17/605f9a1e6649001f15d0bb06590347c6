		                y++;
		            }
		        }
		        answer = p==y ? true: false;
		       

		        return answer;
		    }
	}
	
	static class Solution3{
		public double solution(int[] arr) {
			double answer = 0;
			
			return answer;
		}
	}
	
	public static void main(String[] args) {
	
	}
}
